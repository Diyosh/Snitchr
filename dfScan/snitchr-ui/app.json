{
  "expo": {
    "name": "snitchr-ui",
    "slug": "snitchr-ui",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-fullscreen.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-fullscreen.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "splash": {
      "image": "./assets/images/splash-fullscreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#e9eaea"
    },

    "plugins": [
      "expo-router"
    ],

    "experiments": {
      "typedRoutes": true
    }
  }
}
